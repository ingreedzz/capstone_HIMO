<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Curhat Submissions</title>
  <style>
    body {
      font-family: Arial, sans-serif;
      margin: 20px;
    }
    table {
      width: 100%;
      border-collapse: collapse;
      margin-top: 20px;
    }
    th, td {
      border: 1px solid #ddd;
      padding: 8px;
      text-align: left;
    }
    th {
      background-color: #f4f4f4;
    }
  </style>
</head>
<body>
  <h1>Curhat Submissions</h1>
  <table>
    <thead>
      <tr>
        <th>Text</th>
        <th>Analysis</th>
        <th>Keywords</th>
        <th>Stress Level</th>
      </tr>
    </thead>
    <tbody>
      <% submissions.forEach(submission => { %>
        <tr>
          <td><%= submission.text %></td>
          <td><%= submission.analysis %></td>
          <td><%= submission.keywords.join(", ") %></td>
          <td><%= submission.stressLevel %>%</td>
        </tr>
      <% }) %>
    </tbody>
  </table>
</body>
</html>